    "@types/randomstring": "^1.1.6",
    "@types/simple-oauth2": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "autoprefixer": "9.8.6",
    "aws-sdk": "^2.666.0",
    "axios": "^0.21.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
