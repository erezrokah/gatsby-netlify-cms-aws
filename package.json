{
  "version": "3.0.0",
  "name": "gatsby-netlify-cms-aws",
  "description": "A minimal, lightweight and mobile-first starter for creating blazing-fast static blogs",
  "main": "n/a",
  "private": true,
  "scripts": {
    "develop": "yarn run clean && node --max-old-space-size=8192 ./node_modules/.bin/gatsby develop",
    "build": "yarn run clean && node --max-old-space-size=8192 ./node_modules/.bin/gatsby build",
    "create-cert": "sls create-cert",
    "deploy": "sls deploy",
    "publish": "sls publishSite",
    "clean": "gatsby clean",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint:js": "eslint --cache --ext .js,.jsx .",
    "lint:ts": "eslint --cache --ext .ts,.tsx .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:ts\" \"yarn run lint:scss\" \"yarn flow\"",
    "test": "run-p -l test:frontend test:backend",
    "test:ci": "run-p -l 'test:frontend --runInBand --no-cache' 'test:backend --runInBand --no-cache'",
    "test:coverage": "run-p -l 'test:frontend --coverage' 'test:backend --coverage'",
    "test:frontend": "TZ=GMT CI=true JEST_JUNIT_OUTPUT_DIR=jest/reports JEST_JUNIT_OUTPUT_NAME=${npm_package_name}-test-frontend-results.xml jest --config ./jest/jest-config.js",
    "test:backend": "TZ=GMT CI=true JEST_JUNIT_OUTPUT_DIR=jest/reports JEST_JUNIT_OUTPUT_NAME=${npm_package_name}-test-backend-results.xml jest --config ./backend/configs/jest.config.js",
    "test:watch": "run-p -l 'test:frontend --watch' 'test:backend --watch'",
    "release": "standard-version",
    "prettier": "prettier --write \"{flow,jest,serverless-scripts,serverless-single-page-app-plugin,src,backend,gatsby}/**/*.{js,jsx,ts,scss}\" *.js",
    "prettier:ci": "prettier --list-different \"{flow,jest,serverless-scripts,serverless-single-page-app-plugin,src,backend,gatsby}/**/*.{js,jsx,ts,scss}\" *.js",
    "circleci:setup": "node .circleci/circleci.js setup",
    "circleci:remove": "node .circleci/circleci.js remove",
    "lighthouse": "TZ=GMT CI=true JEST_JUNIT_OUTPUT=jest/reports/${npm_package_name}-lighthouse-results.xml jest --config ./jest/lighthouse/jest-config.js"
  },
  "repository": "https://github.com/erezrokah/gatsby-netlify-cms-aws",
  "author": "Erez Rokah <erezrokah@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "netlify-cms",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme",
    "starter",
    "disqus"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "codecov": "^3.7.1",
    "gatsby": "^2.21.5",
    "gatsby-image": "^2.4.0",
    "gatsby-link": "^2.4.0",
    "gatsby-plugin-catch-links": "^2.3.0",
    "gatsby-plugin-feed": "^2.5.0",
    "gatsby-plugin-flow": "^1.3.0",
