{
  "version": "3.0.0",
  "name": "gatsby-netlify-cms-aws",
  "description": "A minimal, lightweight and mobile-first starter for creating blazing-fast static blogs",
  "main": "n/a",
  "private": true,
  "scripts": {
    "develop": "yarn run clean && node --max-old-space-size=8192 ./node_modules/.bin/gatsby develop",
    "build": "yarn run clean && node --max-old-space-size=8192 ./node_modules/.bin/gatsby build",
    "create-cert": "sls create-cert",
    "deploy": "sls deploy",
    "publish": "sls publishSite",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint:js": "eslint --cache --ext .js,.jsx .",
    "lint:ts": "eslint --cache --ext .ts,.tsx .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:ts\" \"yarn run lint:scss\" \"yarn flow\"",
    "test": "TZ=GMT CI=true JEST_JUNIT_OUTPUT=jest/reports/${npm_package_name}-test-results.xml jest --config ./jest/jest-config.js",
    "test:coverage": "TZ=GMT CI=true jest --coverage --config ./jest/jest-config.js",
    "test:watch": "TZ=GMT CI=true jest --watch --config ./jest/jest-config.js",
    "release": "standard-version",
    "prettier": "prettier --write \"{flow,jest,serverless-scripts,serverless-single-page-app-plugin,src,authServer,gatsby}/**/*.{js,jsx,ts,scss}\" *.js",
    "prettier:ci": "prettier --list-different \"{flow,jest,serverless-scripts,serverless-single-page-app-plugin,src,authServer,gatsby}/**/*.{js,jsx,ts,scss}\" *.js",
    "circleci:setup": "node .circleci/circleci.js setup",
    "circleci:remove": "node .circleci/circleci.js remove",
    "lighthouse": "TZ=GMT CI=true JEST_JUNIT_OUTPUT=jest/reports/${npm_package_name}-lighthouse-results.xml jest --config ./jest/lighthouse/jest-config.js"
  },
  "repository": "https://github.com/erezrokah/gatsby-netlify-cms-aws",
  "author": "Erez Rokah <erezrokah@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "netlify-cms",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme",
    "starter",
    "disqus"
  ],
